    <div class="form-container">
<form action="submit-post" method="post">
    <div class="form-group">
        <label class="form-label" for="name">Name:</label>
        <input class="form-input" type="text" id="name" name="name" required #asp-for="Name">
    </div>
    <div class="form-group">
        <label class="form-label" for="bio">Bio:</label>
        <textarea class="form-input" id="bio" name="bio" required #asp-for="Bio"></textarea>
    </div>
    <div class="form-group">
        <label class="form-label" for="experience">Experience:</label>
        <textarea class="form-input" id="experience" name="experience" required #asp-for="Experience"></textarea>
    </div>
    <div class="form-group">
        <label class="form-label">Availability:</label>
        <div>
            <input type="radio" id="availability-available" name="availability" value="available" required #asp-for="Availability">
            <label for="availability-available">Available</label>
        </div>
        <div>
            <input type="radio" id="availability-contracted" name="availability" value="contracted" required #asp-for="Availability">
            <label for="availability-contracted">Contracted</label>
        </div>
    </div>
    <input class="submit-button" type="submit" value="Submit">
</form>
